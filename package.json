{
  "name": "esbuild-node-builtin",
  "version": "0.0.2",
  "description": "esbuild plugin let u use node builtin modules",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "inject"
  ],
  "scripts": {
    "prepare": "husky install",
    "test": "NODE_OPTIONS='--loader ts-node/esm' mocha",
    "test-cover": "nyc --reporter=lcov --reporter=text mocha",
    "dev": "tsc -w",
    "build": "rm -rf dist && tsc",
    "prepublishOnly": "pnpm build",
    "---": "---",
    "dev:tsup": "tsup src/index.ts --dts --format esm,cjs --watch",
    "build:tsup": "tsup src/index.ts --dts --format esm,cjs"
  },
  "keywords": [
    "esbuild",
    "node",
    "node-builtin",
    "node-polyfills",
    "polyfills-node",
    "polyfills",
    "browserify"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "homepage": "https://github.com/magicdawn/esbuild-node-builtin",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/esbuild-node-builtin.git"
  },
  "bugs": {
    "url": "https://github.com/magicdawn/esbuild-node-builtin/issues"
  },
  "packageManager": "pnpm@7.17.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "rollup": "^3.5.0",
    "rollup-plugin-polyfill-node": "^0.11.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.19",
    "@types/debug": "^4.1.7",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.9",
    "@magicdawn/prettier-config": "^0.0.2",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "p-defer": "3",
    "prettier": "^2.7.1",
    "should": "^13.2.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3"
  },
  "peerDependencies": {
    "esbuild": "*"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md,json}": [
      "prettier --write"
    ]
  }
}
