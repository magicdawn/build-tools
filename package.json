{
  "name": "esbuild-node-builtin",
  "version": "0.0.2",
  "description": "esbuild plugin let u use node builtin modules",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "inject"
  ],
  "scripts": {
    "prepare": "husky install",
    "test": "vitest run",
    "test:dev": "vitest",
    "test-cover": "vitest run --coverage",
    "dev": "tsc -w",
    "build": "rm -rf dist && tsc",
    "prepublishOnly": "pnpm build",
    "---": "---",
    "dev:tsup": "tsup src/index.ts --dts --format esm,cjs --watch",
    "build:tsup": "tsup src/index.ts --dts --format esm,cjs"
  },
  "keywords": [
    "esbuild",
    "node",
    "node-builtin",
    "node-polyfills",
    "polyfills-node",
    "polyfills",
    "browserify"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "homepage": "https://github.com/magicdawn/esbuild-node-builtin",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/esbuild-node-builtin.git"
  },
  "bugs": {
    "url": "https://github.com/magicdawn/esbuild-node-builtin/issues"
  },
  "packageManager": "pnpm@8.6.7",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "rollup": "^3.26.2",
    "rollup-plugin-polyfill-node": "^0.12.0"
  },
  "devDependencies": {
    "@magicdawn/prettier-config": "^0.0.2",
    "@swc/core": "^1.3.68",
    "@types/debug": "^4.1.8",
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^0.33.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "p-defer": "3",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "esbuild": "*"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md,json}": [
      "prettier --write"
    ]
  }
}
